<div class="page-header">
  <h3>Office Profile #<%= @office_profile.patient.first_name %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">


      <dt>Patient</dt>
      <dd><%= @office_profile.patient.first_name %></dd>

      <dt>Birthdate</dt>
      <dd><%= @office_profile.birthdate %></dd>

      <dt>Gender</dt>
      <dd><%= @office_profile.gender %></dd>

      <dt>Phone mobile</dt>
      <dd><%= @office_profile.phone_mobile %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/office_profiles" class="btn btn-primary">
        Back
      </a>
      <a href="/office_profiles/<%= @office_profile.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_office_profile/<%= @office_profile.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
