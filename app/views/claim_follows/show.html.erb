<div class="page-header">
  <h3>Claim Follow #<%= @claim_follow.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Patient request</dt>
      <dd><%= @claim_follow.patient_request %></dd>

      <dt>Doctor start</dt>
      <dd><%= @claim_follow.doctor_start %></dd>

      <dt>Doctor sign</dt>
      <dd><%= @claim_follow.doctor_sign %></dd>

      <dt>Insurance read</dt>
      <dd><%= @claim_follow.insurance_read %></dd>

      <dt>Insurance approved</dt>
      <dd><%= @claim_follow.insurance_approved %></dd>

      <dt>Insurance comments</dt>
      <dd><%= @claim_follow.insurance_comments %></dd>

      <dt>Claim</dt>
      <dd><%= @claim_follow.claim_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/claim_follows" class="btn btn-primary">
        Back
      </a>
      <a href="/claim_follows/<%= @claim_follow.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_claim_follow/<%= @claim_follow.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
