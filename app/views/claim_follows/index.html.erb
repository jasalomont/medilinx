<div class="page-header">
  <h1>
    Claim follows
    <a href="/claim_follows/new" class="btn btn-lg btn-success">New Claim follow</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Patient request</th>
        <th>Doctor start</th>
        <th>Doctor sign</th>
        <th>Insurance read</th>
        <th>Insurance approved</th>
        <th>Insurance comments</th>
        <th>Claim</th>
        <th>Actions</th>
      </tr>

      <% @claim_follows.each do |claim_follow| %>
      <tr>
        <td><%= claim_follow.patient_request %></td>
        <td><%= claim_follow.doctor_start %></td>
        <td><%= claim_follow.doctor_sign %></td>
        <td><%= claim_follow.insurance_read %></td>
        <td><%= claim_follow.insurance_approved %></td>
        <td><%= claim_follow.insurance_comments %></td>
        <td><%= claim_follow.claim_id %></td>
        <td>
          <a href="/claim_follows/<%= claim_follow.id %>" class="btn btn-primary">Show</a>
          <a href="/claim_follows/<%= claim_follow.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_claim_follow/<%= claim_follow.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
