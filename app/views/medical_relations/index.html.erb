<div class="page-header">
  <h1>
    Medical relations
    <a href="/medical_relations/new" class="btn btn-lg btn-success">New Medical relation</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Doctor</th>
        <th>Patient</th>
        <th>Actions</th>
      </tr>

      <% @medical_relations.each do |medical_relation| %>
      <tr>
        <td><%= medical_relation.doctor_id %></td>
        <td><%= medical_relation.patient_id %></td>
        <td>
          <a href="/medical_relations/<%= medical_relation.id %>" class="btn btn-primary">Show</a>
          <a href="/medical_relations/<%= medical_relation.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_medical_relation/<%= medical_relation.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
