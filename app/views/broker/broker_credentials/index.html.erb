<div class="page-header">
  <h1>
    Broker credentials
    <a href="/broker_credentials/new" class="btn btn-lg btn-success">New Broker credential</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Phone work</th>
        <th>Address work</th>
        <th>City work</th>
        <th>Zip work</th>
        <th>State work</th>
        <th>Country work</th>
        <th>Phone mobile</th>
        <th>Practice name</th>
        <th>Company</th>
        <th>Insurer</th>
        <th>Photo</th>
        <th>Job title</th>
        <th>Actions</th>
      </tr>

      <% @broker_credentials.each do |broker_credential| %>
      <tr>
        <td><%= broker_credential.phone_work %></td>
        <td><%= broker_credential.address_work %></td>
        <td><%= broker_credential.city_work %></td>
        <td><%= broker_credential.zip_work %></td>
        <td><%= broker_credential.state_work %></td>
        <td><%= broker_credential.country_work %></td>
        <td><%= broker_credential.phone_mobile %></td>
        <td><%= broker_credential.practice_name %></td>
        <td><%= broker_credential.company %></td>
        <td><%= broker_credential.insurer_id %></td>
        <td><%= broker_credential.photo %></td>
        <td><%= broker_credential.job_title %></td>
        <td>
          <a href="/broker_credentials/<%= broker_credential.id %>" class="btn btn-primary">Show</a>
          <a href="/broker_credentials/<%= broker_credential.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_broker_credential/<%= broker_credential.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
