<div class="page-header">
  <h3>Registro No Patol√≥gico</h3>
</div>

<div class="row">
  <div class="col-md-8 mb-2">
    <dl class="dl-horizontal">
      
      <dt>Doctor</dt>
      <dd><%= @non_pathological.doctor.practice_name %></dd>

      <dt>Fecha de Registro</dt>
      <dd><%= @non_pathological.created_at.to_date %></dd>

      <dd><%= @non_pathological.patient.first_name %></dd>
      <dt>Tipo de consumo</dt>
      <dd><%= @non_pathological.drug_type %></dd>

      <dt>Detalles</dt>
      <dd><%= @non_pathological.quantity %></dd>


    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/historial_medico" class="btn btn-primary">
        Regresar
      </a>
      <% if current_doctor != nil %>

        <a href="/non_pathologicals/<%= @non_pathological.id %>/edit" class="btn btn-warning">
          Editar
        </a>
        <a href="/delete_non_pathological/<%= @non_pathological.id %>" class="btn btn-danger" rel="nofollow">
          Eliminar
        </a>
      <% end %>
    </div>
  </div>
</div>
